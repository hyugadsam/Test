@model IEnumerable<WebApplication1.ServiceReference.User>

<p style="margin-top:20px">
    @Html.ActionLink("Create New", "Edit", new { id = 0})
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BreakFast)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstLastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InsertDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Roleid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Salary)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Savings)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SecondLastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserLogin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.isActive)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BreakFast)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstLastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InsertDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Roleid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Salary)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Savings)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SecondLastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserLogin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.isActive)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Userid })
        </td>
    </tr>
}

</table>
